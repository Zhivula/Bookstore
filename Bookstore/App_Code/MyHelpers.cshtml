@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        @helper CreateListUL(string[] items)
        {
        <div>
            <ul>
                @foreach (var item in items)
                {
                    <li>@item</li>
                }
            </ul>
        </div>
        }
    </div>
</body>
</html>
